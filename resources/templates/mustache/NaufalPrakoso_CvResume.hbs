<script type="text/javascript" src="/resForAriaji/js/jquery-migrate-1.2.1.min.js"></script>

<script src="/resForAriaji/js/jquery.flexslider.js"></script>
<script src="/resForAriaji/js/waypoints.js"></script>
<script src="/resForAriaji/js/jquery.fittext.js"></script>
<script src="/resForAriaji/js/magnific-popup.js"></script>
<script src="/resForAriaji/js/init.js"></script>

</body>

</html>