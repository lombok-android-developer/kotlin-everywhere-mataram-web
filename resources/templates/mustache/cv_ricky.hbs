<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>{{user.name}}</title>

  <!-- Bootstrap core CSS -->
  <link href="/resRicky/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

  <!-- Custom fonts for this template -->
  <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:500,700" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Muli:400,400i,800,800i" rel="stylesheet">
  <link href="/resRicky/vendor/fontawesome-free/css/all.min.css" rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="/resRicky/css/resume.css" rel="stylesheet">

</head>

<body id="page-top">

  <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav">
    <a class="navbar-brand js-scroll-trigger" href="#page-top">
      <span class="d-block d-lg-none">{{user.firstName}}</span>
      <span class="d-none d-lg-block">
        <img class="img-fluid img-profile rounded-circle mx-auto mb-2" src="{{user.photoUrl}}" alt="">
      </span>
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav">
        <li class="nav-item">
          <a class="nav-link js-scroll-trigger" href="#about">About</a>
        </li>
        <li class="nav-item">
          <a class="nav-link js-scroll-trigger" href="#experience">Experience</a>
        </li>
        <li class="nav-item">
          <a class="nav-link js-scroll-trigger" href="#education">Education</a>
        </li>
        <li class="nav-item">
          <a class="nav-link js-scroll-trigger" href="#skills">Skills</a>
        </li>
      </ul>
    </div>
  </nav>

  <div class="container-fluid p-0">

    <section class="resume-section p-3 p-lg-5 d-flex align-items-center" id="about">
      <div class="w-100">
        <h1 class="mb-0">{{user.name}}
          <span class="text-primary">{{user.name}}</span>
        </h1>
        <div class="subheading mb-5">{{user.location}} Â·
          <a href="mailto:name@email.com">{{user.email}}</a>
        </div>
        <p class="lead mb-5">{{user.descriptions}}</p>
        <div class="social-icons">
          <a href="{{user.linkedin}}">
            <i class="fab fa-linkedin-in"></i>
          </a>
          <a href="https://github.com/search?q={{user.idGithub}}">
            <i class="fab fa-github"></i>
          </a>
          <a href="https://t.me/{{user.idTelegram}}">
            <i class="fab fa-telegram"></i>
          </a>
    
        </div>
      </div>
    </section>

    <hr class="m-0">

    <section class="resume-section p-3 p-lg-5 d-flex justify-content-center" id="experience">
      <div class="w-100">
        <h2 class="mb-5">Experience</h2>

        <div class="resume-item d-flex flex-column flex-md-row justify-content-between mb-5">
        {{#user.jobHistory}}
          <div class="resume-content">
            <h3 class="mb-0">{{position}}</h3>
            <div class="subheading mb-3">{{company}}</div>
            <p>{{jobDesc}}</p>
          </div>
          <div class="resume-date text-md-right">
            <span class="text-primary">{{startYears}} - {{endYears}}</span>
          </div>
        </div>
        {{/user.jobHistory}}
      </div>

    </section>

    <hr class="m-0">

    <section class="resume-section p-3 p-lg-5 d-flex align-items-center" id="education">
      <div class="w-100">
        <h2 class="mb-5">Education</h2>
            {{#user.educations}}
        <div class="resume-item d-flex flex-column flex-md-row justify-content-between mb-5">
          <div class="resume-content">
            <h3 class="mb-0">{{institution}}</h3>
            <div class="subheading mb-3">{{descriptions}}</div>
            <div>Computer Science - Software Development Track</div>
          </div>
          <div class="resume-date text-md-right">
            <span class="text-primary">{{grade}} - {{years}}</span>
          </div>
        </div>
          {{/user.educations}}

      </div>
    </section>

    <hr class="m-0">

    <section class="resume-section p-3 p-lg-5 d-flex align-items-center" id="skills">
      <div class="w-100">
        <h2 class="mb-5">Skills</h2>

        <div class="subheading mb-3">Programming Languages &amp; Tools</div>
        <ul class="list-inline dev-icons">
          <li class="list-inline-item">
            <i class="fab fa-html5"></i>
          </li>
          <li class="list-inline-item">
            <i class="fab fa-css3-alt"></i>
          </li>
          <li class="list-inline-item">
            <i class="fab fa-js-square"></i>
          </li>
          <li class="list-inline-item">
            <i class="fab fa-laravel"></i>
          </li>
          <li class="list-inline-item">
            <i class="fab fa-php"></i>
          </li>
          <li class="list-inline-item">
            <i class="fab fa-java"></i>
          </li>
          <li class="list-inline-item">
            <i class="fab fa-wordpress"></i>
          </li>
        </ul>

        <ul class="fa-ul mb-0">
             {{#user.skillset}}
                <li>
                <i class="fa-li fa fa-check"></i>{{name}}
                </li>
            {{/user.skillset}}
        </ul>
      </div>
    </section>

  
  </div>

  <!-- Bootstrap core JavaScript -->
  <script src="/resRickyvendor/jquery/jquery.min.js"></script>
  <script src="/resRickyvendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Plugin JavaScript -->
  <script src="/resRickyvendor/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for this template -->
  <script src="/resRickyjs/resume.min.js"></script>

</body>

</html>
